/**
 * 4-way multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way {
    IN a, b, c, d, sel[2];
    OUT out;

    PARTS:
    Mux (a=a, b=c, sel=sel[1], out=ac);
    Mux (a=b, b=d, sel=sel[1], out=bd);
    Mux (a=ac,b=bd, sel=sel[0], out=out);
}