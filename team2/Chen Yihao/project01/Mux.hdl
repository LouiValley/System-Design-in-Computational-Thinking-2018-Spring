/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not (in=sel, out=nsel);
    And (a=a, b=nsel, out=w1);
    And (a=sel, b=b, out=w2);
    Or  (a=w1, b=w2, out=out);
}