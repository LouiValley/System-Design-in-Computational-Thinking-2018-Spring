/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=Notsel);
    And(a=a,b=Notsel,out=outa);
    And(a=sel,b=b,out=outb);
    Or(a=outa,b=outb,out=out);
}
