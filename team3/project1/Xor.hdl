/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=nota);  
    Not(in=b,out=notb);  
    And(a=nota,b=b,out=o1);  
    And(b=notb,a=a,out=o2);  
    Or(a=o1,b=o2,out=out);  
}