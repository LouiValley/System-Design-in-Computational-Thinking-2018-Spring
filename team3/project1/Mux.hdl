/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=sel,b=b,out=bb);  
    Not(in=sel,out=notsel);  
    And(a=notsel,b=a,out=aa);  
    Or(a=aa,b=bb,out=out); 
}