/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in = b, out = nb);
	Not(in = a, out = na);
	
	And(a = a, b = nb, out = out1);
	And(a = b, b = na, out = out2);
	Or(a = out1, b = out2, out = out);
}